  "description": "React Native Architecture for innovation",
  "license": "GPL",
  "node-version": "16.13.0",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": "~16.13.0"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:assembly": "cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios",
    "ios:8": "npx react-native run-ios --simulator='iPhone 8'",
    "ios:podInstall": "cd ios && pod install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "clean": "npm run clean",
    "build:ios": "react-native run-ios --configuration=release",
    "build:android": "npm run android:clean && npm run android:assembly && react-native run-android --variant=release",
    "update:browser": "npx browserslist@latest --update-db",
    "link": "npx react-native link"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-base64": "^0.2.1",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-simple-toast": "^1.1.3",
    "react-native-uuid": "^2.0.1",
    "react-native-webview": "^11.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.10",
    "@types/react-native-base64": "^0.2.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.1-rc.0-next-3dc41d8a2-20211223",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
